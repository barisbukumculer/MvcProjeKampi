@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<h2>Hakkında Veri Listesi</h2>
<br />
<table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Hakkımda Başlığı</th>
            <th>Aktif Yap</th>
            <th>Pasif Yap</th>
        </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutID</th>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-warning">Aktif Yap</a></td>
            <td><a href="#" class="btn btn-warning">Pasif Yap</a></td>
            
        </tr>
    }
</table>
@Html.Action("AboutPartial","About")